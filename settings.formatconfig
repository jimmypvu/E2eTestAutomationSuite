root = true

[*.*]
enable = true
command = Edit.FormatDocument Analyze.RunDefaultCodeCleanupOnSolution
allowed_extensions = .cs .nunit.cs
denied_extensions = .js .html

[Makefile]
indent_style = tab
tab_width = 2
trim_trailing_whitespace = true
end_of_line = crlf

[**/usings.cs]
generated_code = true